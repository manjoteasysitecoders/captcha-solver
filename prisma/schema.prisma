generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  createdAt DateTime @default(now())
  password  String
  credits   Int      @default(0)
  apiKey    String?
}

model Plan {
  id          String  @id @default(cuid())
  name        String
  price       Int
  credits     Int
  description String?
}
